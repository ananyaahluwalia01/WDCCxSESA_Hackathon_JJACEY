{
  "name": "quacker",
  "description": "reduces your productivity by 100%!",
  "version": "2.0",
  "manifest_version": 2,
  "background": {
    "scripts": ["background.js"],
    "persistent": true
  },
  "permissions": [
    "activeTab",
    "tabs",
    "<all_urls>"
  ],
  "content_scripts": [
    {
      "js": ["inject.js"],
      "matches": ["*://*/*"]
    }
  ],
  "browser_action": {
    "default_icon": "quacker.PNG"
  }
}