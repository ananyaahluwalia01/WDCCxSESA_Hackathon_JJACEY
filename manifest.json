{
  "name": "quacker",
  "description": "reduces your productivity by 100%!",
  "version": "2.0",
  "manifest_version": 2,
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": [
    "storage",
    "activeTab",
    "scripting",
    "webRequest",
    "webRequestBlocking",
    "host",
    "<all_urls>"
  ],
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "quacker.PNG"
  }
}
